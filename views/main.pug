doctype html
html
  head
    link(rel="stylesheet", href="/stylesheets/style.css")
    link(rel="stylesheet", href="/stylesheets/main.css")
    include partials/appName
  body
    div(class="background")
      div(class="container")
        header(class="mainHeader")
          a(href="/myVotings")
            button(class="myVotingBtn" type="button" alt="profile") 🙍‍♀️
          span(class="mainTitle") BLUE VOTE
          a(href="/votings/new")
